    <script>
        function authenticate() {
            const password = document.getElementById("password").value;
            const correctPassword = "your-secure-password"; // 設定するパスワード
            if (password === correctPassword) {
                window.location.href = "https://chatgpt.com/g/g-673d327c89e08191b75af22f5690ae13-cai-yong-asisuto"; // GPTsのURL
            } else {
                alert("Incorrect password. Please try again.");
            }
        }
    </script>
</head>
<body>
    <h1>Protected GPT Access</h1>
    <p>Enter the password to access the GPT:</p>
    <input type="password" id="password" placeholder="Enter Password">
    <button onclick="authenticate()">Submit</button>
